{
  "name": "groq-debugger",
  "displayName": "Groq Debugger",
  "version": "1.0.1",
  "description": "A VSCode extension for debugging with Groq",
  "publisher": "flashboss",
  "icon": "vige.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "activationEvents": [
    "onCommand:groq.debugger"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "contributes": {
    "commands": [
      {
        "command": "groq.debugger",
        "title": "Groq Debugger: Add Breakpoint"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flashboss/groq-debugger.git"
  }
}
